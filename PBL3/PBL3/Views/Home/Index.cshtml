@using PBL3.Models;
@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<table>
    <thead>
        <tr>
            <th>id</th>
            <th>name</th>
            <th>sex</th>
            <th>phone</th>
            <th>address</th>
            <th>username</th>
            <th>email</th>
            <th>password</th>
            <th>status</th>
            <th>role_id</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.id</td>
                <td>@item.name</td>
                <td>@item.sex</td>
                <td>@item.phone</td>
                <td>@item.address</td>
                <td>@item.username</td>
                <td>@item.email</td>
                <td>@item.password</td>
                <td>@item.status</td>
                <td>@item.Role.id</td>
            </tr>
        }
    </tbody>
</table>

